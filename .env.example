# Copia este archivo a .env y configura tus variables

# Configuración para Gemini (recomendado por Midscene)
OPENAI_BASE_URL="https://generativelanguage.googleapis.com/v1beta/openai/"
OPENAI_API_KEY=tu-gemini-api-key-aqui
MIDSCENE_MODEL_NAME="gemini-2.5-pro"
MIDSCENE_USE_GEMINI=1

# Configuración opcional de Puppeteer
PUPPETEER_HEADLESS=false
PUPPETEER_DEVTOOLS=false

# Configuración de timeout (en milisegundos)
NAVIGATION_TIMEOUT=30000
EXTRACTION_TIMEOUT=60000
